<?xml version="1.0" encoding="utf-8"?>
<urls xmlns="http://jelix.org/ns/urls/1.0">
    <classicentrypoint name="index" default="true" optionalTrailingSlash="true">
        <url pathinfo="/" module="adminui" action="default:index"/>
        <url pathinfo="/test" module="test" action="default:index"/>
        <url pathinfo="/auth" module="authcore" include="urls.xml"/>
        <url pathinfo="/test/yes/signin" module="test" action="alwaysyes:signin"/>
        <url pathinfo="/authlogin/" module="authloginpass" include="urls.xml"/>
    </classicentrypoint>
</urls>
